VertexlitGeneric {
	$baseTexture "models/workshop/weapons/c_models/c_heavy_duty_sticky_launcher/c_heavy_duty_sticky_launcher"

	$phong                 1
	$phongboost            3
	$phongexponent         5
	$phongfresnelranges    "[.3 .5 3]"
	$basemapalphaphongmask 1

	$lightwarptexture "models/lightwarps/weapon_lightwarp"

	$rimlight         1
	$rimlightexponent 20
	$rimlightboost    .4

	// Cloaking
	$cloakPassEnabled 1

	// Killstreak
	$sheenPassEnabled   1
	$sheenmap           "cubemaps/cubemap_sheen001"
	$sheenmapmask       "effects/animatedsheen/animatedsheen0"
	$sheenmaptint       "[1 1 1]"
	$sheenmapmaskframe  0
	$sheenindex         0

	// Crits
	$glowcolor "[1 1 1]"

	// Jarate
	$yellow "[1 1 1]"

	Proxies {
		// Cloaking
		invis {}

		// Killstreak
		AnimatedWeaponSheen {
			animatedtexturevar         $sheenmapmask
			animatedtextureframenumvar $sheenmapmaskframe
			animatedtextureframerate   40
		}

		// Crits
		ModelGlowColor {
			resultVar $glowcolor
		}

		Equals {
			srcVar1   $glowcolor
			resultVar $selfillumtint
		}

		// Jarate
		YellowLevel {
			resultVar $yellow
		}

		// Crit or Jarate
		Multiply {
			srcVar1   $glowcolor
			srcVar2   $yellow
			resultVar $color2
		}
	}
}
