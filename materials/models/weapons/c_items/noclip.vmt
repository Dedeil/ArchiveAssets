VertexLitGeneric {
	$basetexture "models/weapons/c_items/noclip"

	$phong                 1
	$phongboost            4
	$phongexponent         5
	$phongfresnelranges    "[.3 1 8]"
	$basemapalphaphongmask 1

	$lightwarptexture "models/lightwarps/weapon_lightwarp"

	$rimlight         1
	$rimlightexponent 4
	$rimlightboost    .5

	// Cloaking
	$cloakPassEnabled 1

	// Killstreak
	$sheenPassEnabled  1
	$sheenmap          "cubemaps/cubemap_sheen001"
	$sheenmapmask      "effects/animatedsheen/animatedsheen0"
	$sheenmaptint      "[1 1 1]"
	$sheenmapmaskframe 0
	$sheenindex        0

	// Crits
	$glowcolor "[1 1 1]"

	// Jarate
	$yellow "[1 1 1]"

	// Community weapon
	$tempvar               0
	$basephongexponent     5
	$basephongboost        4
	$commadd_phongexponent 12
	$commadd_phongboost    22.5
	$commweapon            0

	Proxies {
		// Cloaking
		invis {}

		// Killstreak
		AnimatedWeaponSheen {
			animatedtexturevar         $sheenmapmask
			animatedtextureframenumvar $sheenmapmaskframe
			animatedtextureframerate   40
		}

		// Crits
		ModelGlowColor {
			resultVar $glowcolor
		}

		Equals {
			srcVar1   $glowcolor
			resultVar $selfillumtint
		}

		// Jarate
		YellowLevel {
			resultVar $yellow
		}

		// Crit or Jarate
		Multiply {
			srcVar1   $glowcolor
			srcVar2   $yellow
			resultVar $color2
		}

		// Community weapon
		CommunityWeapon {
			resultVar $commweapon
		}

		Multiply {
			srcVar1   $commweapon
			srcVar2   $commadd_phongexponent
			resultVar $tempvar
		}

		Add {
			srcVar1   $tempvar
			srcVar2   $basephongexponent
			resultVar $phongexponent
		}

		Multiply {
			srcVar1   $commweapon
			srcVar2   $commadd_phongboost
			resultVar $tempvar
		}

		Add {
			srcVar1   $tempvar
			srcVar2   $basephongboost
			resultVar $phongboost
		}
	}
}
